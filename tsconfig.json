{
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": true,
		"jsx": "react",
		"module": "es6",
		"moduleResolution": "node",
		"target": "es6",
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}
